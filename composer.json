{
    "name": "gyaaniguy/starter-basic",
    "type": "library",
    "description": "Basic template composer Codeception",
    "authors": [
        {
            "name": "Nikhil",
            "email": "nik.jain@gmail.com"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "gyaani\\guy\\": "src",
            "gyaani\\guy\\Classes\\": "src/Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gyaani\\guy\\": "tests"
        }
    },
    "bin": ["vendor/bin"],
    "scripts": {
        "test": "codecept",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}